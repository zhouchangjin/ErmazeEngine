<ui>
<window id="guide_menu">
	<rect x="40" y="40" w="17" h="16" num_type="percentage" radius="10">
			<background color="(88,120,64,200)" />			
			<border type="box" width="5" color="(136,176,96,240)"/>
			<font color="(255,255,255)"/>
			<container layout="grid" row="3" col="2">
				<button icon="" action="package_window">包裹</button>
				<button icon="" action="equip_menu">装备</button>
				<button icon="" action="strong_window">强度</button>
				<button icon="" action="border_window">乘降</button>
				<button icon="" action="skill_window">特技</button>
				<button icon="" action="guide_window">向导</button>
			</container>
	</rect>
</window>

<window id="equip_menu">
	<rect x="40" y="40" w="17" h="16" num_type="percentage" radius="10">
		<background color="(88,120,64,200)" />			
		<border type="box" width="5" color="(136,176,96,240)"/>
		<font color="(255,255,255)"/>	
		<container layout="grid">
			<col>
				{player.name}
			</col>
			<col>
				{rank.name}
			</col>
		</container>
	</rect>
</window>

</ui>